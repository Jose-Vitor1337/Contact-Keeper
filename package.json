{
  "name": "contact-keeper",
  "version": "1.0.0",
  "description": "Contack menager app",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "server": "nodemon ./src/server.js",
    "client": "npm start --prefix frontend",
    "client-install": "npm install --prefix frontend",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "dependencies-backend": "npm install express bcryptjs jsonwebtoken config express-validator mongoose",
    "dependencies-typescript": "npm install --save-dev @types/express-validator @types/jsonwebtoken @types/express @types/bcrypt @types/mongoose typescript typescript",
    "dependencies-development": "npm install --save-dev nodemon concurrently"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express-validator": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "git": "^0.1.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.5.17",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.3"
  }
}
